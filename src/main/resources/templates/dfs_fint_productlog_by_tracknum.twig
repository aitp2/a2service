{
  "version": true,
  "_source": [ "logsource","logtime", "productCode" ,"message"],
  "query": {
    "bool": {
      "must": [
        {
          "query_string": {
            "analyze_wildcard": true,
            "query": "productCode:*{{productCode}}* AND {{pendingtypeCondition}}"
          }
        },
          {
          "range": {
            "logtime.keyword": {
              "gte": "{{starttime}}",
              "lt": "{{endtime}}"
            }
          }
        }
      ],
      "must_not": []
    }
  },
   "sort": { "logtime.keyword": { "order": "desc" } }
}